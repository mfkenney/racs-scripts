# -*- mode: shell-script -*-
#
# Sampling interval in seconds
RACS_INTERVAL=21600
# Maximum number of seconds allowed for PPP connection
RACS_PPP_LINKTIME=300
# Maximum time in seconds to keep the PPP link up
RACS_PPP_TIMELIMIT=900
# NTP server hostname or IP address
RACS_NTP_SERVER=140.142.1.8
# NTP sync timeout in seconds
RACS_NTP_TIMEOUT=10
# FTP server hostname or IP address
RACS_FTP_SERVER=128.95.96.18
# User account on the FTP server
RACS_FTP_USER=ftpuser
# Rescale factor for uploaded images
RACS_SCALE=0.5
# Number of cameras
RACS_NCAMERAS=2
# Camera boot time in seconds
RACS_CAMERA_BOOTTIME=30
# Extra camera warm-up time in seconds
RACS_CAMERA_WARMUP=0
# Number of seconds to run the video stream 1 is usually
# sufficient but this may need to be increased in low
# light conditions
RACS_STREAM_TIME=1
# Maximum age (in days) of files in the OUTBOX
RACS_MAX_AGE=3
# By default, the oldest file in the OUTBOX is uploaded
# first. Setting this variable to any non-null value will
# cause the newest file to uploaded first.
RACS_REV_SORT=
# A/D sampling interval (value and units)
RACS_ADC_INTERVAL=5s
# Camera power switches (must be an array)
RACS_CAMERA_POWER=( 8160_LCD_D2 8160_LCD_D3 )
# Ethernet power
RACS_ENET_POWER=8160_LCD_D1
# Iridium modem power
RACS_MODEM_POWER=8160_LCD_D0
